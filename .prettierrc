{
	"useTabs": true,
	"singleQuote": true,
	"semi": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"printWidth": 100,
	"proseWrap": "always",
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
