{
	"name": "busser",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"cloudsql:proxy": "cross-env-shell \"./cloud-sql-proxy $INSTANCE_CONNECTION_NAME\"",
		"migrate:core": "tsx node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts --env core",
		"migrate:user": "tsx node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts --env user",
		"migrate:rollback:core": "tsx node_modules/knex/bin/cli.js migrate:rollback --knexfile knexfile.ts --env core",
		"migrate:rollback:user": "tsx node_modules/knex/bin/cli.js migrate:rollback --knexfile knexfile.ts --env user",
		"migrate:status:core": "tsx node_modules/knex/bin/cli.js migrate:status --knexfile knexfile.ts --env core",
		"migrate:status:user": "tsx node_modules/knex/bin/cli.js migrate:status --knexfile knexfile.ts --env user",
		"seed:core": "tsx node_modules/knex/bin/cli.js seed:run --knexfile knexfile.ts --env core",
		"seed:user": "tsx node_modules/knex/bin/cli.js seed:run --knexfile knexfile.ts --env user",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@flydotio/dockerfile": "^0.7.8",
		"@prgm/sveltekit-progress-bar": "^3.0.2",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/bcrypt": "^5.0.2",
		"@types/crypto-js": "^4.2.2",
		"@types/jsonwebtoken": "^9.0.9",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"autoprefixer": "^10.4.19",
		"cross-env": "^10.1.0",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.5",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.2.0",
		"postcss": "^8.4.40",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.6",
		"sass": "^1.72.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-eslint-parser": "^1.4.1",
		"tailwindcss": "^3.4.7",
		"tslib": "^2.4.1",
		"tsx": "^4.21.0",
		"typescript": "^5.5.0",
		"vite": "^6.2.4",
		"vitest": "^4.0.18"
	},
	"type": "module",
	"dependencies": {
		"@dicebear/collection": "^9.3.1",
		"@dicebear/core": "^9.3.1",
		"@google-cloud/aiplatform": "^6.2.0",
		"@google-cloud/storage": "^7.12.0",
		"@internationalized/date": "^3.10.1",
		"bcrypt": "^5.1.1",
		"bits-ui": "^1.0.0-next.98",
		"change-case": "^5.4.4",
		"clsx": "^2.1.1",
		"crypto-js": "^4.2.0",
		"dicebear": "^9.3.1",
		"fast-json-stringify": "^5.15.0",
		"form-data": "^4.0.5",
		"http-status-codes": "^2.3.0",
		"jsonwebtoken": "^9.0.2",
		"knex": "^3.1.0",
		"knex-paginate": "^3.1.4",
		"knex-types": "^0.5.0",
		"lucide-svelte": "^0.562.0",
		"mailgun.js": "^12.0.1",
		"micromatch": "^4.0.8",
		"moment": "^2.30.1",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.13.0",
		"openai": "^4.61.1",
		"svelte-dnd-action": "^0.9.69",
		"svelte-guard": "^1.0.0",
		"svelte-infinite": "^0.5.0",
